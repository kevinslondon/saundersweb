{% extends 'base.html.twig' %}

{% form_theme form _self %}



{% block text_label %}
    <div class="label_col">
        {{ block('form_label') }}
    </div>


{% endblock %}

{% block text_widget %}
    <div class="form_field">
        {{ block('form_widget_simple') }}
    </div>
    <br clear="left" />

{% endblock %}

{% block submit_widget %}
    <div class="label_col">
       &nbsp;
    </div>
    <div class="form_field">
        <input type="submit" value="Submit" class="form_submit">
    </div>
    <br clear="left" />

{% endblock %}

{% block title %}Saunders Web Solutions: Java/Spring or PHP/MySQL, Laravel,Zend Web Development{% endblock %}

{% block body %}
    <section class="clearfix">
        <h1>Saunders Web Solutions: Java/Spring or PHP/MySQL, Laravel,Zend Web Development</h1>
    </section>

    <section>
        <h3>Contact Me</h3>

        {{ form_start(form) }}
        {{ form_widget(form) }}
        {{ form_end(form) }}

    </section>

    <section>

    </section>


{% endblock %}

{% block stylesheets %}
    <style>

    </style>
{% endblock %}
